<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smsaz.fitnessenthusiast">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".UserEditScreenActivity"></activity>
        <activity android:name=".faq.FAQActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ExercieList" />
        </activity>
        <activity android:name=".BodyPartActivity" />
        <activity android:name=".ExercieList" />
        <activity
            android:name=".ExerciseActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".NutritionGuidelines" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ExercieList" />
        </activity>
        <activity android:name=".MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.view.LoginActivity">
        </activity>
        <activity android:name=".signup.SignupActivity" />
    </application>

</manifest>